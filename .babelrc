{
  "presets": ["es2015"],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "utils": "./src/utils",
        "constants": "./src/constants",
        "globals": "./src/globals",
        "tests": "./src/tests"
      }
    }],
    "transform-object-rest-spread"
  ]
}